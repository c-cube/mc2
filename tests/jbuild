
; vim:ft=lisp:

(jbuild_version 1)

(executable
  ((name test_api)
   (libraries (dolmen containers sequence
               mc2.core mc2.dimacs mc2.backend))
   (flags (:standard -w +a-4-42-44-48-50-58-32-60@8 -color always))
   (ocamlopt_flags (:standard -O3
                    -unbox-closures -unbox-closures-factor 20))
  ))

(alias
 ((name    runtest)
  (deps    (test_api.exe))
  (action  (run ${<}))))

